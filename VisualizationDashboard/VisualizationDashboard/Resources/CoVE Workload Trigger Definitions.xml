		<!-- Utility Measures-->		
		<!-- Frequency/Division-->
		<Measurement ID="Frequency" Continuity="Continuous">
			<Description>Gets the frequency in minutes</Description>
			<Parameter Name="Count" Type="Int32"/>
			<Constant Name="msToMin" Type="Int32" Value="6000"/>
			<MeasurementComputation Operator="Divide">
				<ParameterRef Name="Count"/>
				<MeasurementComputation Operator="Divide">
					<Timing Type="TotalRunningTime"/>
					<ConstantRef Name="msToMin"/>
				</MeasurementComputation>
			</MeasurementComputation>
		</Measurement>
		
