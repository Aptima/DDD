<Window x:Class="Decision_Aid.SelectObjectDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Select Object Dialog" Height="154" Width="368" WindowStartupLocation="CenterOwner" WindowStyle="ToolWindow">
    <Grid Height="117">
        <Label Content="Select an object from the drop down below:" Height="28" HorizontalAlignment="Left" Margin="12,12,0,0" Name="label1" VerticalAlignment="Top" />
        <ComboBox Height="23" HorizontalAlignment="Left" Margin="12,46,0,0" Name="comboBoxObjects" VerticalAlignment="Top" Width="242" />
        <Button Content="Ok" Height="23" HorizontalAlignment="Right" Margin="0,0,92,12" Name="buttonOk" VerticalAlignment="Bottom" Width="75" Click="buttonOk_Click" />
        <Button Content="Cancel" Margin="0,0,12,12" Name="buttonCancel" Height="23" VerticalAlignment="Bottom" HorizontalAlignment="Right" Width="75" Click="buttonCancel_Click" />
    </Grid>
</Window>
